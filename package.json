{
  "name": "ethereum-validators-monitoring",
  "version": "4.7.0",
  "main": "./dist/main.js",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_OPTIONS=--max_old_space_size=8192 node dist/src/main --trace-warnings --sync --sub-fin-head",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_OPTIONS=--experimental-vm-modules jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "author": "Lido",
  "license": "MIT",
  "description": "Consensus layer validators monitoring bot, that fetches Lido or User Node Operator keys from Execution layer and checks their performance in Consensus layer by: balance delta, attestations, proposals, sync committee participation.",
  "dependencies": {
    "@chainsafe/persistent-merkle-tree": "^0.7.1",
    "@chainsafe/ssz": "^0.9.4",
    "@clickhouse/client": "^0.0.11",
    "@ethersproject/bignumber": "^5.4.1",
    "@lido-nestjs/execution": "^1.14.1",
    "@lido-nestjs/logger": "^1.3.2",
    "@lido-nestjs/registry": "^7.11.1",
    "@lodestar/types": "^1.34.1",
    "@mikro-orm/core": "^5.3.1",
    "@mikro-orm/knex": "^5.3.1",
    "@mikro-orm/nestjs": "^5.1.0",
    "@mikro-orm/sqlite": "^5.3.1",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.20",
    "@nestjs/platform-fastify": "^10.4.20",
    "@nestjs/terminus": "^10.3.0",
    "@types/js-yaml": "^4.0.5",
    "@types/validator": "^13.7.1",
    "@willsoto/nestjs-prometheus": "^5.5.1",
    "app-root-path": "^3.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.14.0",
    "dotenv": "^10.0.0",
    "fp-ts": "^2.11.5",
    "got-cjs": "^12.5.4",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "prom-client": "^14.2.0",
    "reflect-metadata": "^0.1.13",
    "retry-ts": "^0.1.3",
    "rxjs": "^7.1.0",
    "stream-chain": "^2.2.5",
    "stream-json": "^1.7.5",
    "typechain": "^5.2.0",
    "undici": "^6.21.2"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.7",
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^10.4.20",
    "@types/app-root-path": "^1.2.4",
    "@types/cache-manager": "^3.4.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom":"^29.7.0",
    "mocha": "^10.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.1",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typechain": "^5.2.0",
    "typescript": "^5.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^build-info$": "<rootDir>/../build-info.json"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}
