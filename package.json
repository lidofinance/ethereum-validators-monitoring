{
  "name": "ethereum-validators-monitoring",
  "version": "1.3.2",
  "main": "./dist/main.js",
  "private": true,
  "scripts": {
    "build": "npm run typechaingen && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main --max-old-space-size=8192 --trace-warnings --sync --sub-fin-head",
    "test": "echo 'No tests yet'",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typechaingen": "typechain --target=ethers-v5 --out-dir ./src/ethereum/execution/generated './src/ethereum/execution/abi/*.json'"
  },
  "author": "Lido",
  "license": "MIT",
  "description": "Consensus layer validators monitoring bot, that fetches Lido Node Operator keys from Execution layer and checks their performance in Consensus layer by: balance delta, attestations, proposes, sync committee participation.",
